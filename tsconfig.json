{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "CommonJS",
    "target": "ES2015",
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "strict": true,
    "noImplicitOverride": true,
    // "noPropertyAccessFromIndexSignature": true,
    // "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "dist/*",
    // "examples/*",
    // "test/*",
    "test-dist/*"
  ]
}